{
  "name": "copilot-browser-vscode",
  "displayName": "Copilot Browser",
  "description": "Connect your browser to VS Code & AI Copilot for automated browser control, testing, and intelligent web automation",
  "version": "2.1.6",
  "publisher": "jaypal-browser-copilot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaypal1046/copilot-browser"
  },
  "homepage": "https://github.com/jaypal1046/copilot-browser#readme",
  "icon": "image.jpg",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Testing",
    "Other",
    "Debuggers"
  ],
  "keywords": [
    "browser",
    "automation",
    "copilot",
    "testing",
    "web",
    "selenium",
    "puppeteer"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "browserAgent.connect",
        "title": "Browser Agent: Connect to Relay Server",
        "category": "Browser Agent"
      },
      {
        "command": "browserAgent.disconnect",
        "title": "Browser Agent: Disconnect",
        "category": "Browser Agent"
      },
      {
        "command": "browserAgent.launchBrowser",
        "title": "Browser Agent: Launch Isolated Browser",
        "category": "Browser Agent"
      },
      {
        "command": "browserAgent.navigate",
        "title": "Browser Agent: Navigate to URL",
        "category": "Browser Agent"
      },
      {
        "command": "browserAgent.click",
        "title": "Browser Agent: Click Element",
        "category": "Browser Agent"
      },
      {
        "command": "browserAgent.type",
        "title": "Browser Agent: Type Text",
        "category": "Browser Agent"
      },
      {
        "command": "browserAgent.getConsole",
        "title": "Browser Agent: Get Console Logs",
        "category": "Browser Agent"
      },
      {
        "command": "browserAgent.getDOM",
        "title": "Browser Agent: Get DOM",
        "category": "Browser Agent"
      },
      {
        "command": "browserAgent.screenshot",
        "title": "Browser Agent: Take Screenshot",
        "category": "Browser Agent"
      },
      {
        "command": "browserAgent.executeJS",
        "title": "Browser Agent: Execute JavaScript",
        "category": "Browser Agent"
      },
      {
        "command": "browserAgent.getPerformance",
        "title": "Browser Agent: Get Performance Metrics",
        "category": "Browser Agent"
      },
      {
        "command": "browserAgent.listTabs",
        "title": "Browser Agent: List All Tabs",
        "category": "Browser Agent – Tabs"
      },
      {
        "command": "browserAgent.openTab",
        "title": "Browser Agent: Open New Tab",
        "category": "Browser Agent – Tabs"
      },
      {
        "command": "browserAgent.closeTab",
        "title": "Browser Agent: Close Tab",
        "category": "Browser Agent – Tabs"
      },
      {
        "command": "browserAgent.switchTab",
        "title": "Browser Agent: Switch to Tab",
        "category": "Browser Agent – Tabs"
      },
      {
        "command": "browserAgent.reloadTab",
        "title": "Browser Agent: Reload Tab",
        "category": "Browser Agent – Tabs"
      },
      {
        "command": "browserAgent.findByText",
        "title": "Browser Agent: Find Element by Text",
        "category": "Browser Agent – Smart Select"
      },
      {
        "command": "browserAgent.findByRole",
        "title": "Browser Agent: Find Element by Role",
        "category": "Browser Agent – Smart Select"
      },
      {
        "command": "browserAgent.findByLabel",
        "title": "Browser Agent: Find Element by Label",
        "category": "Browser Agent – Smart Select"
      },
      {
        "command": "browserAgent.waitForSelector",
        "title": "Browser Agent: Wait for Selector",
        "category": "Browser Agent – Wait"
      },
      {
        "command": "browserAgent.waitForText",
        "title": "Browser Agent: Wait for Text",
        "category": "Browser Agent – Wait"
      },
      {
        "command": "browserAgent.summarizePage",
        "title": "Browser Agent: Summarize Page",
        "category": "Browser Agent – AI"
      },
      {
        "command": "browserAgent.getAccessibility",
        "title": "Browser Agent: Accessibility Audit",
        "category": "Browser Agent – AI"
      },
      {
        "command": "browserAgent.keyPress",
        "title": "Browser Agent: Press Key",
        "category": "Browser Agent – Keyboard"
      },
      {
        "command": "browserAgent.keyCombo",
        "title": "Browser Agent: Key Combination",
        "category": "Browser Agent – Keyboard"
      },
      {
        "command": "browserAgent.getNetwork",
        "title": "Browser Agent: Get Network Logs",
        "category": "Browser Agent"
      },
      {
        "command": "browserAgent.getStats",
        "title": "Browser Agent: Get Stats",
        "category": "Browser Agent"
      },
      {
        "command": "browserAgent.visualSnapshot",
        "title": "Browser Agent: Take Visual Snapshot",
        "category": "Browser Agent – AI"
      },
      {
        "command": "browserAgent.startRecording",
        "title": "Browser Agent: Start Recording",
        "category": "Browser Agent – Recording"
      },
      {
        "command": "browserAgent.stopRecording",
        "title": "Browser Agent: Stop Recording",
        "category": "Browser Agent – Recording"
      },
      {
        "command": "browserAgent.getRecording",
        "title": "Browser Agent: Get Recording",
        "category": "Browser Agent – Recording"
      },
      {
        "command": "browserAgent.runTests",
        "title": "Browser Agent: Run Integration Tests",
        "category": "Browser Agent"
      }
    ],
    "configuration": {
      "title": "Copilot Browser",
      "properties": {
        "browserAgent.relayServerUrl": {
          "type": "string",
          "default": "ws://localhost:8080",
          "description": "WebSocket URL of the relay server"
        },
        "browserAgent.autoConnect": {
          "type": "boolean",
          "default": false,
          "description": "Automatically connect to relay server on startup"
        },
        "browserAgent.commandTimeout": {
          "type": "number",
          "default": 30000,
          "description": "Timeout for browser commands (ms)"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "browserAgent",
          "title": "Browser Agent",
          "icon": "resources/browser-agent.svg"
        }
      ]
    },
    "views": {
      "browserAgent": [
        {
          "type": "webview",
          "id": "browserAgentView",
          "name": "Browser Agent",
          "icon": "resources/browser-agent.svg"
        }
      ]
    }
  },
  "scripts": {
    "package": "npx -y @vscode/vsce package",
    "build": "echo 'No build step required'",
    "watch": "echo 'No watch step required'",
    "lint": "eslint .",
    "test": "node test-integration.js"
  },
  "dependencies": {
    "puppeteer-core": "^21.5.2",
    "relay-server": "file:relay-server-1.0.0.tgz",
    "uuid": "^9.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "eslint": "^8.50.0"
  }
}